    @RenderBody()

    <section class="download section-padding" style="background-color: #26272b; padding:0; margin:0; padding-top: 30px;">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="sectioner-header text-center white">
                        <h3>THANKS FOR VISITING US</h3>
                    </div>
                </div>
            </div>
        </div>
    </section>
